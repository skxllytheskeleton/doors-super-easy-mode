GFM.Parent = workspace.CurrentRooms["0"]
game:GetService("ReplicatedStorage").Entities.Screech.Top.Slime_Top.Transparency = 1
game:GetService("Workspace").CurrentRooms["0"].Assets["Desk_Bell"].Base.InteractSound.SoundId = "rbxassetid://11984351"
game:GetService("Workspace").CurrentRooms["0"].Assets["Desk_Bell"].Base.InteractSound.PlaybackSpeed = 1
game:GetService("Workspace").CurrentRooms["0"].Assets["Desk_Bell"].Base.InteractSound.Volume = 10
game:GetService("Workspace").CurrentRooms["0"].Assets["Desk_Bell"].InteractPrompt.Triggered:Connect(function()
    game.Players.LocalPlayer.Character.Humanoid.Health = 0
end)
Mouse.KeyDown:Connect(function(key)
    if key == "q" then
        Sprinting = true
    end
end)
Mouse.KeyUp:Connect(function(key)
    if key == "q" then
        Sprinting = false
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 15
    end
end)
--entity spawning
repeat
    wait(0.1)
until game:GetService("ReplicatedStorage").GameData.LatestRoom.Value > 3
    game:GetService("ReplicatedStorage").GameData.LatestRoom.Changed:Connect(function()
        if Woebegone == true and WoebegoneProgress == 4 then
            Woebegone = false
            WoebegoneProgress = 0
            if HasWoebegoneAchievement == false then
                local Achievements = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Custom%20Achievements/Source.lua"))()
                
                -- Creates and displays your custom achievement
                Achievements.Get({
                    Title = "Guiltlessness",
                    Desc = "Unkind of you...",
                    Reason = "Survive Woebegone",
                    Image = "https://cdn.discordapp.com/attachments/1080299690328858655/1103823433046958214/120.png",
                })
                HasWoebegoneAchievement = true
            end
        end
        if Woebegone == true then

        end
        whisperer.Enabled = false
        WhispererTalking:Stop()
        local SpawnChance = math.random(1,20)
        if SpawnChance >= 8 then
            local EntityToSpawn = math.random(1,23)
            if EntityToSpawn == 1 then
            local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/Source.lua"))()
            
            local entity = Spawner.createEntity({
            CustomName = "SUBSPACE TRIPMINE!!!!",
            Model = "rbxassetid://13232317657", -- Your entity's model url here ("rbxassetid://1234567890" or GitHub raw url)
                Speed = 300,
                MoveDelay = 2,
                HeightOffset = 0,
                CanKill = true,
                KillRange = 50,
                SpawnInFront = false,
                ShatterLights = false,
                FlickerLights = {
                    Enabled = true,
                    Duration = 1
                },
                Cycles = {
                    Min = 2,
                    Max = 5,
                    Delay = 0
                },
                CamShake = {
                    Enabled = false,
                    Values = {900, 900, 900, 900},
                    Range = 100
                },
                ResistCrucifix = false,
                BreakCrucifix = true,
                DeathMessage = {"You died to subspace tripmine...", "hide like you do with rush", "idiot"},
                IsCuriousLight = true
            })
            
            ---====== Debug ======---
            
            entity.Debug.OnEntitySpawned = function()
                print("Entity has spawned")
            end
            
            entity.Debug.OnEntityDespawned = function()
                print("Entity has despawned")
            end
            
            entity.Debug.OnEntityStartMoving = function()
                print("Entity started moving")
            end
            
            entity.Debug.OnEntityFinishedRebound = function()
                print("Entity finished rebound")
            end
            
            entity.Debug.OnEntityEnteredRoom = function(room)
                print("Entity entered room:", room)
            end
            
            entity.Debug.OnLookAtEntity = function()
                print("Player looking at entity")
            end
            
            entity.Debug.OnDeath = function()
                print("Player has died")
            end
            
            --[[
                NOTE: By overwriting 'OnUseCrucifix', the default crucifixion will be ignored and this function will be called instead
            
                entity.Debug.OnUseCrucifix = function()
                    print("Custom crucifixion script here")
                end
            ]]--
            Spawner.runEntity(entity)
            end            ---====== Create entity ======---
            
   
            if EntityToSpawn == 2 then
        
                local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/Source.lua"))()
                
