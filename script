                    })
                    
                    ---====== Debug ======---
                    
                    entity.Debug.OnEntitySpawned = function()
                        print("Entity has spawned")
                    end
                    
                    entity.Debug.OnEntityDespawned = function()
                        print("Entity has despawned")
                    end
                    
                    entity.Debug.OnEntityStartMoving = function()
                        print("Entity started moving")
                    end
                    
                    entity.Debug.OnEntityFinishedRebound = function()
                        print("Entity finished rebound")
                    end
                    
                    entity.Debug.OnEntityEnteredRoom = function(room)
                        print("Entity entered room:", room)
                    end
                    
                    entity.Debug.OnLookAtEntity = function()
                        print("Player looking at entity")
                    end
                    
                    entity.Debug.OnDeath = function()
                        print("Player has died")
                    end
                    
                    --[[
                        NOTE: By overwriting 'OnUseCrucifix', the default crucifixion will be ignored and this function will be called instead
                    
                        entity.Debug.OnUseCrucifix = function()
                            print("Custom crucifixion script here")
                        end
                    ]]--
                    
                    ---====== Run entity ======---
                    
                    Spawner.runEntity(entity)
                end
                if EntityToSpawn == 19 then
                    whisperer.Enabled = true
                    WhispererTalking:Play()
                end
                if EntityToSpawn == 20 then
                    local a = require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game.RemoteListener.Modules["A90"])(require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game))()         
                end
                if EntityToSpawn == 21 then
                    require(game.ReplicatedStorage.ClientModules.EntityModules.Shade).stuff(require(game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game),workspace.CurrentRooms[game.Players.LocalPlayer:GetAttribute("CurrentRoom")])         
                end
                if EntityToSpawn == 22 then
                    local EntitySpawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/dreadmania/Scripts/main/Spawner_V2.lua"))()
                    local Configuration = {
                        Kill = false, -- change to "Damage = 10," for eyes, doesnt work on other entities
                        Speed = 160, -- 60 for rush, doesnt work on other entities
                        Time = 3 -- 5 for rush, doesnt work on other entities
                    }
                     
                    EntitySpawner:Spawn("Jack", Configuration)
                end
                if EntityToSpawn == 23 then
        
                    local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/Source.lua"))()
                    
                    ---====== Create entity ======---
                    
                    local entity = Spawner.createEntity({
                        CustomName = "B-60",
                        Model = "rbxassetid://13336898692", -- Your entity's model url here ("rbxassetid://1234567890" or GitHub raw url)
                        Speed = 100,
                        MoveDelay = 2,
                        HeightOffset = 2,
                        CanKill = true,
                        KillRange = 50,
                        SpawnInFront = true,
                        ShatterLights = false,
                        FlickerLights = {
                            Enabled = false,
                            Duration = 1
                        },
                        Cycles = {
                            Min = 1,
                            Max = 1,
                            Delay = 0
                        },
                        CamShake = {
                            Enabled = true,
                            Values = {3.5, 20, 0.1, 1},
                            Range = 100
                        },
                        ResistCrucifix = false,
                        BreakCrucifix = true,
                        DeathMessage = {"You died to B-60...","Wait... What?","How did B-60 make it to the hotel?","The fanmade stuff is getting out of hand...","Well, use the same tactics you do with Rush."},
                        IsCuriousLight = false
                    })
                    
                    ---====== Debug ======---
                    
                    entity.Debug.OnEntitySpawned = function()
                        print("Entity has spawned")
                    end
                    
                    entity.Debug.OnEntityDespawned = function()
                        print("Entity has despawned")
                    end
                    
                    entity.Debug.OnEntityStartMoving = function()
                        print("Entity started moving")
                    end
                    
                    entity.Debug.OnEntityFinishedRebound = function()
                        print("Entity finished rebound")
                    end
                    
                    entity.Debug.OnEntityEnteredRoom = function(room)
                        print("Entity entered room:", room)
                    end
                    
                    entity.Debug.OnLookAtEntity = function()
                        print("Player looking at entity")
                    end
                    
                    entity.Debug.OnDeath = function()
                        print("Player has died")
                    end
                    
                    --[[
                        NOTE: By overwriting 'OnUseCrucifix', the default crucifixion will be ignored and this function will be called instead
                    
                        entity.Debug.OnUseCrucifix = function()
                            print("Custom crucifixion script here")
                        end
                    ]]--
                    
                    ---====== Run entity ======---
                    
                    Spawner.runEntity(entity)
                end
                if EntityToSpawn == 24 and Woebegone == false and WoebegoneProgress == 0 then
        
                    local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/Source.lua"))()
                    
                    local Warning = game:GetObjects("rbxassetid://")[1]
                    local Sound = Instance.new("Sound")
                    Warning.Parent = game.Lighting
                    Sound.SoundId = "rbxassetid://166047422"
                    Sound.Parent = workspace
                    Sound:Play()
                    wait(0.1)
                    Warning:Destroy()
                    wait(1.305)
                    Sound:Destroy()
                    
                    ---====== Create entity ======---
                    
                    local entity = Spawner.createEntity({
                        CustomName = "Woebegone",
                        Model = "rbxassetid://13233447962", -- Your entity's model url here ("rbxassetid://1234567890" or GitHub raw url)
                        Speed = 100,
                        MoveDelay = 0,
                        HeightOffset = 0,
                        CanKill = true,
                        KillRange = 50,
                        SpawnInFront = false,
                        ShatterLights = false,
                        FlickerLights = {
                            Enabled = false,
                            Duration = 1
                        },
                        Cycles = {
                            Min = 1,
                            Max = 1,
                            Delay = 0
                        },
                        CamShake = {
                            Enabled = false,
                            Values = {900, 900, 900, 900},
                            Range = 100
                        },
                        ResistCrucifix = false,
                        BreakCrucifix = true,
                        DeathMessage = {"S̵̼̖͕̮̐̓Ŏ̸̡͉͈͖̈́M̶̟̣̅̒͌E̴͎̞̝̩̤͛T̷͉̉̃̚H̸̪̺̦͇͖̏I̵̧̖͎̍N̸̡̞͈̒G̵̘͍͓̩̗͋͑ ̷̧͂͝B̴͍̾À̵̬̘͝Ḓ̴̠̰͋̇̉̉ ̸̼͇̭̑̅͂̈́W̷̰̅̀̂͘Ỉ̷̖̖͓̺̽̅̕̚L̴̹͚̮̤̈L̵̹̗̝̺͂͂ ̸͖͙͙̪̘̍̋H̶̜͖͖̦̯͋͑̀̉Ȃ̷̛̱̞͝P̸̬̪͊̀̒͋͝P̷̜̯̏E̷̦̼̒N̴̩̲̟̗͕͝ ̵̖̖̲̉́S̴̭̩͓̒̑̆̽̄O̸͍̔̊͗̔O̴̲̖͍͉̊́͛͆̈́N̷͕͙̏̌͊̿̕.̴̓͌͜", "Use the same tactics that you use with Rush and Ambush!"},
                        IsCuriousLight = false
                    })
                    
                    ---====== Debug ======---
                    
                    entity.Debug.OnEntitySpawned = function()
                        print("Entity has spawned")
                    end
                    
                    entity.Debug.OnEntityDespawned = function()
                         WoebegoneProgress = WoebegoneProgress + 1
                    end
                    
                    entity.Debug.OnEntityStartMoving = function()
                        print("Entity started moving")
                    end
                    
                    entity.Debug.OnEntityFinishedRebound = function()
                        print("Entity finished rebound")
                    end
                    
                    entity.Debug.OnEntityEnteredRoom = function(room)
                        print("Entity entered room:", room)
                    end
                    
                    entity.Debug.OnLookAtEntity = function()
                        print("Player looking at entity")
                    end
                    
                    entity.Debug.OnDeath = function()
                        print("Player has died")
                    end
                    
                    --[[
                        NOTE: By overwriting 'OnUseCrucifix', the default crucifixion will be ignored and this function will be called instead
                    
                        entity.Debug.OnUseCrucifix = function()
                            print("Custom crucifixion script here")
                        end
                    ]]--
                    
                    ---====== Run entity ======---
                    
                   Spawner.runEntity(entity)
                end
    end
end)
while true do
    wait(0.1)
    if Sprinting == true then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 21
    end
end
