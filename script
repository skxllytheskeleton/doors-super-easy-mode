--SUPER EASY MODE!!!
-- customization
local SpawnWithCrucifix = false -- set to true if u want (doesnt work on real entities)
-- set-up
local Sprinting = false
local Woebegone = false
local WoebegoneProgress = 0
local Mouse = game:GetService("Players").LocalPlayer:GetMouse()
local Humanoid = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
local HasSilenceAchievement = false
local HasDepthAchievement = false
local HasSneakAttackAchievement = false
local HasSpeedAchievement = false
local HasSurgeAchievement = false
local HasB60Achievement = false
local HasWoebegoneAchievement = false
local SolarisLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/sol"))()
SolarisLib:Notification("super easy mode","it has been turned on!")

local Functions = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Functions.lua"))()
local CustomShop = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Custom%20Shop%20Items/Source.lua"))()


local exampleTool = LoadCustomInstance("rbxassetid://13288743648")


CustomShop.CreateItem(exampleTool, {
    Title = "Guiding Candle",
    Desc = "??? ???? ?? ???? ???",
    Image = "rbxassetid://13288888356",
    Price = 75,
    Stack = 1,
})
exampleTool.Equipped:Connect(function()
    local Idle = exampleTool.Animations.idle
    local IdleTrack = Humanoid:LoadAnimation(Idle)
    IdleTrack:Play()
end)
exampleTool.Unequipped:Connect(function()
    for _,anim in pairs(Humanoid.Animator:GetPlayingAnimationTracks()) do
        anim:Stop()
    end
end)
local whisperer = Instance.new("ScreenGui")
local Whisperer = Instance.new("ImageLabel")
local WhispererTalking = Instance.new("Sound")

whisperer.Name = "whisperer"
whisperer.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
whisperer.Enabled = false
whisperer.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Whisperer.Name = "Whisperer"
Whisperer.Parent = whisperer
Whisperer.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Whisperer.BackgroundTransparency = 1.000
Whisperer.BorderSizePixel = 0
Whisperer.Size = UDim2.new(1, 0, 1, 0)
Whisperer.Image = "http://www.roblox.com/asset/?id=10971355872"

WhispererTalking.Name = "Talking"
WhispererTalking.SoundId = "rbxassetid://1202347858"
WhispererTalking.PlaybackSpeed = 0.5
WhispererTalking.Parent = whisperer
WhispererTalking.Looped = true

local Dread = Instance.new("ScreenGui")
local ImageLabel = Instance.new("ImageLabel")

Dread.Name = "Dread"
Dread.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
Dread.Enabled = false
Dread.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

