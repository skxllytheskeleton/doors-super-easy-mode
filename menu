--super easy entity and item spawner
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "SUPER EASY ENTITY AND ITEM SPAWNER", HidePremium = false, SaveConfig = false})

local Tab = Window:MakeTab({
	Name = "Entities",
	Icon = "rbxassetid://11287256498",
	PremiumOnly = false
})
local Tab2 = Window:MakeTab({
	Name = "Items",
	Icon = "rbxassetid://11374263930",
	PremiumOnly = false
})

local whisperer = Instance.new("ScreenGui")
local Whisperer = Instance.new("ImageLabel")
local WhispererTalking = Instance.new("Sound")

whisperer.Name = "whisperer"
whisperer.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
whisperer.Enabled = false
whisperer.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Whisperer.Name = "Whisperer"
Whisperer.Parent = whisperer
Whisperer.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Whisperer.BackgroundTransparency = 1.000
Whisperer.BorderSizePixel = 0
Whisperer.Size = UDim2.new(1, 0, 1, 0)
Whisperer.Image = "http://www.roblox.com/asset/?id=10971355872"

WhispererTalking.Name = "Talking"
WhispererTalking.SoundId = "rbxassetid://1202347858"
WhispererTalking.PlaybackSpeed = 0.5
WhispererTalking.Parent = whisperer
WhispererTalking.Looped = true

local Dread = Instance.new("ScreenGui")
local ImageLabel = Instance.new("ImageLabel")

Dread.Name = "Dread"
Dread.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
Dread.Enabled = false
Dread.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

ImageLabel.Parent = Dread
ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ImageLabel.BackgroundTransparency = 1.000
ImageLabel.Size = UDim2.new(1, 0, 1, 0)
ImageLabel.Image = "http://www.roblox.com/asset/?id=11036572035"
ImageLabel.ImageTransparency = 0.500

Tab:AddButton({
	Name = "Subspace Tripmine",
	Callback = function()
        local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/Source.lua"))()
            
        local entity = Spawner.createEntity({
        CustomName = "SUBSPACE TRIPMINE!!!!",
        Model = "rbxassetid://13232317657", -- Your entity's model url here ("rbxassetid://1234567890" or GitHub raw url)
            Speed = 300,
            MoveDelay = 2,
            HeightOffset = 0,
            CanKill = true,
            KillRange = 50,
            SpawnInFront = false,
            ShatterLights = false,
            FlickerLights = {
                Enabled = true,
                Duration = 1
            },
            Cycles = {
                Min = 2,
                Max = 5,
                Delay = 0
            },
            CamShake = {
                Enabled = false,
                Values = {900, 900, 900, 900},
                Range = 100
            },
            ResistCrucifix = false,
            BreakCrucifix = true,
            DeathMessage = {"You died to subspace tripmine...", "hide like you do with rush", "idiot"},
            IsCuriousLight = true
        })
        
        ---====== Debug ======---
        
        entity.Debug.OnEntitySpawned = function()
            print("Entity has spawned")
        end
        
        entity.Debug.OnEntityDespawned = function()
            print("Entity has despawned")
        end
        
        entity.Debug.OnEntityStartMoving = function()
            print("Entity started moving")
        end
        
        entity.Debug.OnEntityFinishedRebound = function()
            print("Entity finished rebound")
        end
        
        entity.Debug.OnEntityEnteredRoom = function(room)
            print("Entity entered room:", room)
        end
        
        entity.Debug.OnLookAtEntity = function()
            print("Player looking at entity")
        end
        
        entity.Debug.OnDeath = function()
            print("Player has died")
        end
        
        --[[
            NOTE: By overwriting 'OnUseCrucifix', the default crucifixion will be ignored and this function will be called instead
        
            entity.Debug.OnUseCrucifix = function()
                print("Custom crucifixion script here")
            end
        ]]--
        Spawner.runEntity(entity)
  	end    
})
Tab:AddButton({
	Name = "Dread",
	Callback = function()
        local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/Source.lua"))()
                
        Dread.Enabled = true
        ---====== Create entity ======---
        
        local entity = Spawner.createEntity({
            CustomName = "Dread",
            Model = "rbxassetid://12409937936", -- Your entity's model url here ("rbxassetid://1234567890" or GitHub raw url)
            Speed = 100,
            MoveDelay = 2,
            HeightOffset = 0,
            CanKill = true,
            KillRange = 50,
            SpawnInFront = false,
            ShatterLights = false,
            FlickerLights = {
                Enabled = true,
                Duration = 1
            },
            Cycles = {
                Min = 1,
                Max = 1,
                Delay = 0
            },
            CamShake = {
                Enabled = false,
                Values = {900, 900, 900, 900},
                Range = 100
            },
            ResistCrucifix = false,
            BreakCrucifix = true,
            DeathMessage = {"You died to...", "Uh...", "I'm not sure who you died to.","...","It looks terrifying so you could call him Dread.","Use the same tactics that you use with Rush and Ambush!"},
            IsCuriousLight = false
        })
        
        ---====== Debug ======---
        
        entity.Debug.OnEntitySpawned = function()
            print("Entity has spawned")
        end
        
        entity.Debug.OnEntityDespawned = function()
            Dread.Enabled = false
        end
        
        entity.Debug.OnEntityStartMoving = function()
            print("Entity started moving")
        end
        
        entity.Debug.OnEntityFinishedRebound = function()
            print("Entity finished rebound")
        end
        
        entity.Debug.OnEntityEnteredRoom = function(room)
            print("Entity entered room:", room)
        end
        
        entity.Debug.OnLookAtEntity = function()
            print("Player looking at entity")
        end
        
        entity.Debug.OnDeath = function()
            print("Player has died")
        end
        
        --[[
            NOTE: By overwriting 'OnUseCrucifix', the default crucifixion will be ignored and this function will be called instead
        
            entity.Debug.OnUseCrucifix = function()
                print("Custom crucifixion script here")
            end
        ]]--
        
        ---====== Run entity ======---
        
        Spawner.runEntity(entity)
  	end    
})
Tab:AddButton({
	Name = "Kenny",
	Callback = function()
        local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/Source.lua"))()
        local Warning = game:GetObjects("rbxassetid://13317095429")[1]
        local Sound = Instance.new("Sound")
        local Reverb = Instance.new("ReverbSoundEffect")
        Reverb.Parent = Sound
        Sound.Volume = 5
        Sound.Parent = workspace
        Sound.SoundId = "rbxassetid://5000061542"
        Sound:Play()
        Warning.Parent = game.Lighting
        Warning.Saturation = -1
        wait(3.577)
        Warning:Destroy()
        Sound:Destroy()
        ---====== Create entity ======---
        
        local entity = Spawner.createEntity({
            CustomName = "Kenny",
            Model = "rbxassetid://13234153006", -- Your entity's model url here ("rbxassetid://1234567890" or GitHub raw url)
            Speed = 1000,
            MoveDelay = 2,
            HeightOffset = 0,
            CanKill = true,
            KillRange = 50,
            SpawnInFront = false,
            ShatterLights = true,
            FlickerLights = {
                Enabled = false,
                Duration = 3
            },
            Cycles = {
                Min = 1,
                Max = 1,
                Delay = 0
            },
            CamShake = {
                Enabled = true,
                Values = {0.1, 900, 900, 900},
                Range = 100
            },
            ResistCrucifix = true,
            BreakCrucifix = true,
            DeathMessage = {"You died to Kenny...", "Use the same tactics that you use with Rush and Ambush!"},
            IsCuriousLight = false
        })
        
        ---====== Debug ======---
        
        entity.Debug.OnEntitySpawned = function()
            print("Entity has spawned")
        end
        
        entity.Debug.OnEntityDespawned = function()
            print("Entity has despawned")
        end
        
        entity.Debug.OnEntityStartMoving = function()
