            DeathMessage = {"SÌµÌÌ“Ì¼Ì–Í•Ì®OÌ¸Ì†Í„Í‰ÍˆÌ¡Í–MÌ¶Ì…Ì’ÍŒÌŸÌ£EÌ´Í›ÍŽÌžÌÌ©Ì¤TÌ·ÌšÌ‰ÌƒÍ‰HÌ¸ÌÌªÌºÌ¦Í‡Í–IÌµÌÌ–Ì§ÍŽNÌ¸Ì’Ì¡ÌžÍˆGÌµÍ‹Í‘Ì˜ÍÍ“Ì©Ì— Ì·ÍÍ‚Ì§BÌ´Ì¾ÍAÌµÍÌ€Ì¬Ì˜DÌ´Í‹Ì‡Ì‰Ì‰Ì­Ì Ì° Ì¸Ì‘Ì…Í‚Í„Ì¼Í‡Ì­WÌ·Ì…Í˜Ì€Ì‚Ì°IÌ·Ì•ÌšÌ‰Ì½Ì…Ì–Ì–Í“ÌºLÌ´ÌˆÌ¹ÍšÌ®Ì¤LÌµÍ‚Í‚Ì¹Ì—ÌÌº Ì¸ÌÌ‹Í–Í™Í™ÌªÌ˜HÌ¶Í‹Í‘Ì€Ì‰ÌœÍ–Í–Ì¦Ì¯AÌ·ÍÌ‘Ì›Ì±ÌžPÌ¸ÍÍŠÌ€Ì’Í‹Ì¬ÌªPÌ·ÌÌœÌ¯EÌ·Ì’Ì¦Ì¼NÌ´ÍÌ©Ì²ÌŸÌ—Í• ÌµÌ‰ÍÌ–Ì–Ì²SÌ´Ì’Ì‘Ì†Ì½Ì„Ì­Ì©Í“OÌ¸Ì”ÌŠÍ—Ì”ÍOÌ´ÌŠÌÍ›Í†Í„Ì²Ì–ÍÍ‰NÌ·ÌÌŒÌ•ÍŠÌ¿Í•Í™.Ì´ÍƒÍŒÍœ", "Use the same tactics that you use with Rush and Ambush!"},
            IsCuriousLight = false
        })
        
        ---====== Debug ======---
        
        entity.Debug.OnEntitySpawned = function()
            print("Entity has spawned")
        end
        
        entity.Debug.OnEntityDespawned = function()

        end
        
        entity.Debug.OnEntityStartMoving = function()
            print("Entity started moving")
        end
        
        entity.Debug.OnEntityFinishedRebound = function()
            print("Entity finished rebound")
        end
        
        entity.Debug.OnEntityEnteredRoom = function(room)
            print("Entity entered room:", room)
        end
        
        entity.Debug.OnLookAtEntity = function()
            print("Player looking at entity")
        end
        
        entity.Debug.OnDeath = function()
            print("Player has died")
        end
        
        --[[
            NOTE: By overwriting 'OnUseCrucifix', the default crucifixion will be ignored and this function will be called instead
        
            entity.Debug.OnUseCrucifix = function()
                print("Custom crucifixion script here")
            end
        ]]--
        
        ---====== Run entity ======---
        
       Spawner.runEntity(entity)
  	end    
})
Tab:AddButton({
	Name = "A-200",
	Callback = function()
        local Spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/Source.lua"))()
                    
        ---====== Create entity ======---
        
        local entity = Spawner.createEntity({
            CustomName = "A-200",
            Model = "rbxassetid://13381226966", -- Your entity's model url here ("rbxassetid://1234567890" or GitHub raw url)
            Speed = 100,
            MoveDelay = 2,
            HeightOffset = 2,
            CanKill = true,
            KillRange = 50,
            SpawnInFront = true,
            ShatterLights = false,
            FlickerLights = {
                Enabled = false,
                Duration = 1
            },
            Cycles = {
                Min = 1,
                Max = 1,
                Delay = 0
            },
            CamShake = {
                Enabled = true,
                Values = {3.5, 20, 0.1, 1},
                Range = 100
            },
            ResistCrucifix = false,
            BreakCrucifix = true,
            DeathMessage = {"You died to A-200...","Wait... What?","How did A-200 make it to the hotel?","Well, use the same tactics you do with Rush."},
            IsCuriousLight = false
        })
        
        ---====== Debug ======---
        
        entity.Debug.OnEntitySpawned = function()
            print("Entity has spawned")
        end
        
        entity.Debug.OnEntityDespawned = function()
            print("Entity has despawned")
        end
        
        entity.Debug.OnEntityStartMoving = function()
            print("Entity started moving")
        end
        
        entity.Debug.OnEntityFinishedRebound = function()
            print("Entity finished rebound")
        end
        
        entity.Debug.OnEntityEnteredRoom = function(room)
            print("Entity entered room:", room)
        end
        
        entity.Debug.OnLookAtEntity = function()
            print("Player looking at entity")
        end
        
        entity.Debug.OnDeath = function()
            print("Player has died")
        end
        
        --[[
            NOTE: By overwriting 'OnUseCrucifix', the default crucifixion will be ignored and this function will be called instead
        
            entity.Debug.OnUseCrucifix = function()
                print("Custom crucifixion script here")
            end
        ]]--
        
        ---====== Run entity ======---
        
        Spawner.runEntity(entity)
  	end    
})
Tab2:AddButton({
	Name = "Guiding Candle",
	Callback = function()
        local Tool = game:GetObjects("rbxassetid://13288743648")[1]
        local Humanoid = game.Players.LocalPlayer.Character.Humanoid

        Tool.Parent = game.Players.LocalPlayer.Backpack
        
        Tool.Equipped:Connect(function()
            local Idle = Tool.Animations.idle
            local IdleTrack = Humanoid:LoadAnimation(Idle)
            IdleTrack:Play()
        end)
        
        Tool.Unequipped:Connect(function()
            for _,v in pairs(Humanoid.Animator:GetPlayingAnimationTracks()) do
                v:Stop()
            end
        end)
  	end    
})
OrionLib:Init()
